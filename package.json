{
  "name": "grid-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "run-p -s -l start:*",
    "start:frontend": "vite --force",
    "start:typechecker": "npm run typecheck:watch",
    "build": "vite build --base ./  --outDir docs",
    "eslint": "run-s -s -n eslint:nofix:*",
    "eslint:nofix:frontend": "eslint ./src/**/*.{js,ts,tsx}",
    "eslint:fix": "run-s -s -n eslint:fix:*",
    "eslint:fix:frontend": "eslint --fix ./src/**/*.{js,ts,tsx}",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "node ./typecheck.js",
    "typecheck:watch": "node ./typecheck_watch.js",
    "format:check": "prettier --check  \"./src/**/*.{js,ts,tsx,json}\"",
    "format:fix": "prettier --write  \"./src/**/*.{js,ts,tsx,json}\""
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@simple-html/grid": "5.0.0-next.96",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/wicg-file-system-access": "2023.10.5",
    "@vitejs/plugin-react": "4.3.3",
    "@sqlite.org/sqlite-wasm": "3.46.0-build2",
    "autoprefixer": "10.4.20",
    "esbuild-helpers": "2.0.1",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "fflate": "0.8.2",
    "globals": "15.11.0",
    "msw": "2.5.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primereact": "10.8.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "6.27.0",
    "react-router-dom": "6.27.0",
    "tailwind-merge": "2.5.4",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0",
    "vite": "5.4.10",
    "vitest": "2.1.3",
    "eslint-plugin-prettier": "5.2.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zustand": "5.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
